  "scripts": {
    "test": "node_modules/.bin/jest --coverage --coverageDirectory=test_results/coverage",
    "snapshot": "node_modules/.bin/jest -u",
    "count": "node_modules/.bin/cloc --exclude-dir=node_modules,target --include-lang=TypeScript,Sass .",
    "start": "node_modules/.bin/http-server ./target -p 3000",
    "build": "npm run-script lint && npm run-script test && npm run-script prod",
    "build-ci": "npm run-script lint-ci && npm run-script test && npm run-script jenkins",
    "jenkins": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/webpack --config webpack.prod.js",
    "prod": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.prod.js",
    "dev": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "lint-ci": "node_modules/.bin/tslint --force -c ./tslint.json -p ./tsconfig.json --formatters-dir ./node_modules/tslint-junit-formatter/formatters --format junit -o ./test_results/tslint.xml {src,__tests__}/**/*.{ts,tsx}",
    "lint": "node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json {src,__tests__}/**/*.{ts,tsx}",
    "proto-js": "node_modules/.bin/pbjs -t static-module -w commonjs -o src/models/example.js  proto/**/*.proto",
    "proto-ts": "node_modules/.bin/pbts -o src/models/example.d.ts src/models/example.js",
    "proto": "npm run-script proto-js && npm run-script proto-ts",
    "deploy": "npm run-script build && gh-pages -d target"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/shim.js",
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "testResultsProcessor": "jest-junit",
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/debug/**",
      "!**/src/**/*.d.ts"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true",
    "output": "test_results/jest.xml"
  }
}
